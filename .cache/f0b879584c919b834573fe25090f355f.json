Y,\n  secret: process.env.API_SECRET\n});\n\nclass Results extends React.Component {\n  state = {\n    pets: []\n  };\n  componentDidMount() {\n    petfinder.pet.find({ find: \"full\", location: \"Seattle, WA\" }).then(data => {\n      let pets;\n      if (data.petfinder.pets && data.petfinder.pets.pet) {\n        //if it exists...\n        if (Array.isArray(data.petfinder.pets.pet)) {\n          // if multiple pets\n          pets = data.petfinder.pets.pet;\n        } else {\n          //if just one, wrap it in an array\n          pets = [data.petfinder.pets.pet];\n        }\n      } else {\n        pets = [];\n      }\n\n      this.setState({ pets });\n    });\n  }\n\n  render() {\n    return (\n      <div className=\"search\">\n        <h5>results search</h5>\n        {this.state.pets.map((pet, idx) => {\n          // often in this API dogs can be more than one breed, therefore necessitating these clauses below\n          let breed;\n          if (Array.isArray(pet.breeds.breed)) {\n            breed = pet.breeds.breed.join(\", \");\n          } else {\n            breed = pet.breeds.breed;\n          }\n          return (\n            <Pet\n              key={idx}\n              media={pet.media}\n              animal={pet.animal}\n              name={pet.name}\n              breed={breed}\n              location={`${pet.contact.city}, ${pet.contact.state}`}\n              id={pet.id}\n            />\n          );\n        })}\n      </div>\n    );\n  }\n}\n\nexport default Results;\n"},"lineCount":null}},"hash":"14f7e1fb089f4509492262638eabaa45","cacheData":{"env":{"API_KEY":"3cb89639c8bafd4e0eb569af5896c230","API_SECRET":"f160a9c4f6ede1b665d2581b470c379c"}}}